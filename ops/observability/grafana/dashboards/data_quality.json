{
  "uid": "data-quality",
  "title": "Data Quality",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "1m",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "DQ Checks by Status (Last 7d)",
      "datasource": "Prometheus",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "sum by (status) (increase(dq_checks_total[7d]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "DQ Failures by Dataset (Last 7d)",
      "datasource": "Prometheus",
      "gridPos": {"x": 0, "y": 6, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "sum by (dataset) (increase(dq_checks_total{status=\"fail\"}[7d]))",
          "refId": "A"
        }
      ]
    }
  ]
}
